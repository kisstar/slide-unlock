{
  "name": "@kisstar/slide-unlock",
  "version": "0.1.0",
  "description": "A JavaScript library that provides sliding unlocking.",
  "main": "dist/slide-unlock.umd.js",
  "module": "dist/slide-unlock.es.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm-run-all -p watch:*",
    "prebuild": "rimraf dist",
    "build": "npm-run-all -p build:*",
    "build:js": "tsc --module commonjs && cross-env NODE_ENV=production rollup -c",
    "build:css": "sass --load-path='./' --no-source-map src/styles/index.scss dist/slide-unlock.css",
    "watch:js": "rollup -c -w",
    "watch:css": "npm run build:css -- --watch",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint . --ext .js,.ts",
    "lint:prettier": "prettier --check \"src/**\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "prerelease": "npm run build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/kisstar/slide-unlock#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisstar/slide-unlock.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "sass": "^1.32.8",
    "standard-version": "^9.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.2.3"
  },
  "author": "kisstar <dwh.chn@foxmail.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "slide-unlock"
  ],
  "lint-staged": {
    "./src/*.{js,ts}": "eslint",
    "./src/*.{css,scss}": "stylelint --syntax=scss",
    "**/*.{js,ts,css,scss,md,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "license": "MIT"
}
